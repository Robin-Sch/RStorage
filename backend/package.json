{
  "name": "rstorage-backend",
  "version": "0.1.0",
  "description": "RStorage is an encrypted cloud file storage.",
  "scripts": {
    "build": "npx tsc",
    "dev": "PANEL_DISABLE_REGISTER=false  tsc-watch --onSuccess \"node dist/src/index.js\"",
    "eslint": "npx eslint src --report-unused-disable-directives",
    "update-deps": "npx ncu -u && npm install && npm audit fix",
    "copy": "cp -r src/public src/views dist/src"
  },
  "dependencies": {
    "bcrypt": "^5.1.1",
    "better-sqlite3": "^11.10.0",
    "express": "^5.1.0",
    "express-session": "^1.18.1",
    "form-data": "^4.0.2",
    "node-fetch": "^3.3.2",
    "socket.io": "^4.8.1",
    "socket.io-stream": "^0.9.1",
    "speakeasy": "^2.0.0",
    "uuid": "^11.1.0"
  }
}
